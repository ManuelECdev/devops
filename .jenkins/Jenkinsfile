#!/usr/bin/env groovy
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
		echo "Running ${env.BUILD_NUMBER} on ${env.JOB_NAME}"
		echo "Running ${env.EXECUTOR_NUMBER} on ${env.BUILD_TAG}"
		echo "Running ${env.WORKSPACE} on ${env.NODE_NAME}"
            }
        }
    }
}
